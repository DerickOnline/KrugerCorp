        <servlet-name>RegUsuario2</servlet-name>
        <url-pattern>/RegUsuario2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActUsuario</servlet-name>
        <url-pattern>/ActUsuario</url-pattern>
