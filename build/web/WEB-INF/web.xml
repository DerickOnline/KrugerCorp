        <servlet-name>RegCursoU</servlet-name>
        <servlet-class>CONTROLADOR.RegCursoU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActCursoU</servlet-name>
        <servlet-class>CONTROLADOR.ActCursoU</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegPersonal</servlet-name>
        <url-pattern>/RegPersonal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegPersonal2</servlet-name>
        <url-pattern>/RegPersonal2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegUsuario</servlet-name>
        <url-pattern>/RegUsuario</url-pattern>
    </servlet-mapping>
